!!!5
html
  head
    - if (locals.pageTitle)
      title= locals.pageTitle
    - else
      title Site Title 

    - stylesheets.forEach(function(s) {
      link(rel="stylesheet", href=s)
    - })
    - if (!inProduction)
      script(src="/ui/scripts/external/live.js")

  body
    != body
    script(src="/ui/scripts/external/ender.min.js")
    - scripts.forEach(function(s) {
      script(src=s)
    - })
